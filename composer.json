{
  "name" : "phil/geolocation-bundle",
  "description" : "A Symfony2 Bundle to handle geographic location. Add geolocation to your entities. Add address entities. Add user geo location. Add doctrine functions for calculating geographical distances in your project.",
  "type" : "symfony-bundle",
  "keywords" : ["symfony2", "geocoding", "geoip", "address", "geocode", "geo location"],
  "license" : ["MIT"],
  "homepage": "https://github.com/ph-il/geolocation-bundle",
  "authors": [
    {
      "name": "Philippe Gamache",
      "email": "philippe@ph-il.ca",
      "homepage": "http://ph-il.ca",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "securesymfony@ph-il.ca",
    "issues": "https://github.com/ph-il/geolocation-bundle/issues",
    "source": "https://github.com/ph-il/geolocation-bundle"
  },
  "require" : {
    "php": ">=5.5.9",
    "symfony/symfony": "3.0.*",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-cache-bundle": "^1.2",
    "doctrine/doctrine-fixtures-bundle": "~2.2",
    "stof/doctrine-extensions-bundle": "^1.1",
    "twig/extensions": "~1.0",
    "sensio/framework-extra-bundle": "^3.0.2",
    "willdurand/geocoder-bundle": "4.*",
    "league/geotools": "^0.3"

  },
  "minimum-stability": "dev",
  "require-dev": {
    "symfony/finder": "~2.1",
    "symfony/browser-kit": "~2.1",
    "satooshi/php-coveralls": "~0.6"
  },
  "autoload" : {
    "psr-0" : {
      "Phil\\GeolocationBundle" : ""
    }
  },
  "target-dir" : "Phil/GeolocationBundle"
}
